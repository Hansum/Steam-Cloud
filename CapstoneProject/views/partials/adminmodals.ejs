				<div class='row'>
					<div class='col-lg-4 text-center'>
						<h4>Name:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.fname %> <%= data.mname %> <%= data.lname %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>Email:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.email %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>Position:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.position %> - <%= data.facultyType %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>Educational Attainment:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.educationalAttainment %> - <%= data.specialization %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>School Graduated:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.schoolGraduated %> - <%= data.yearGraduated %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>Gender:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.gender %></h4>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						<h4>Date Hired:</h4>
					</div>
					<div class='col-lg-8'>
						<h4><%= data.dateHired %></h4>
					</div><br><br>
					<br>
					<div class='col-lg-12 text-center'>
						<h3>Publication</h3><br>
						<h3>Book</h3>
						<h3><% facultyBook.forEach(function(cdata){%>						
							<% if( cdata.facultyId == data.facultyId && cdata.bookVerificationLevel == 1 ){ %>
								<%= cdata.bookTitle %>, <%= cdata.bookEdition %> Edition - <%= cdata.bookYear %> ISBN: <%= cdata.ISBN %><br>
							<% } %>
						<% }); %></h3>
							<h3>Journal</h3>
						<h3><% facultyJournal.forEach(function(adata){%>
							<% if( adata.facultyId == data.facultyId && adata.journalVerificationLevel == 1 ){ %>
								<%= adata.journalTitle %>, <%= adata.journalVolume %> Volume, Link: <%= adata.journalLink %><br>
							<% } %>
						<% }); %></h3>
						<h3>Research</h3>
						<h3><% facultyResearch.forEach(function(bdata){%>
							<% if( bdata.facultyId == data.facultyId && bdata.researchVerificationLevel == 1 ){ %>
								<%= bdata.researchName %> - <%= bdata.researchDescription %>, Co-researchers: <%= bdata.coresearchers %>, Collab.: <%= bdata.collaborations %>, STATUS: <%= bdata.researchStatus %><br>
							<% } %>
						<% }); %></h3>
					</div>

				</div>

			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
<% }); %>

