<!-- INSTITUTION --!>

<div id="editInstitution" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Edit Institution</h4>
			</div>
			<div class="modal-body">
				<form encType="multipart/form-data" class="form-horizontal" action="/editInstitution" method="POST">
					<div class="form-group">
						<div class='col-lg-12 text-center'>
							<img src='../images/institution<%= data[0].institutionId %>.jpg' class='face'>
						</div><br><br>
						<label class="control-label col-lg-3">Profile Image:</label>
						<div class='col-lg-9'>
						<input type='file' class='form-control' name='institutionLogo' accept=".jpg" title='Only .jpg files'>
						</div><br><br>
						<label class="control-label col-lg-3">Current Name:</label>
						<div class="col-lg-9">
							<input type='hidden' value='<%= data[0].institutionId %>' name='institutionId'>
							<p><%= data[0].institutionName%></p>
						</div><br><br>
						<label class="control-label col-lg-3">New Name:</label>
						<div class="col-lg-9">
							<input type="text" class="form-control" id="institutionName" placeholder="Enter name" name="institutionName" required>
						</div><br><br>
						<label class="control-label col-lg-3">Address:</label>
						<div class="col-lg-9">
							<input type="text" class="form-control" id="institutionAddress" placeholder="Enter address" name="institutionAddress" required>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Region:</label>
						<div class="col-lg-9">
							<select class='form-control' name='institutionRegion'>
							<% allRegion.forEach(function(data){ %>
								<option value='<%= data.region_id %>'><%= data.regionName %></option>
								
							<% }); %>
							</select>
						</div>
					</div>
					
				
			</div>
			<div class="modal-footer">
			<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</form></div>
		</div>
	</div>
</div>

