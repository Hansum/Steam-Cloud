<!----Edit Faculty Profile----!>

<div id="editProfile" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Edit Profile</h4>
			</div>
			<div class="modal-body">

				<form class="form-horizontal" action="/editProfileFaculty" method="POST">
					<div class="form-group">
						<label class="control-label col-lg-4">First Name:</label>
						<div class="col-lg-8">
							<input type='hidden' value='<%= data[0].departmentId %>' name='departmentId' >
							<input type='hidden' value='<%= data[0].schoolId %>' name='schoolId' >
							<input type='hidden' value='<%= data[0].institutionId %>' name='institutionId' >
							<input type='hidden' value='<%= data[0].facultyId %>' name='facultyId'>
							<input type="text" class="form-control" name="fname" placeholder="<%= data[0].fname%>" required>
						</div><br><br>
						<label class="control-label col-lg-4">Last Name:</label>
							<div class="col-lg-8">
								<input type="text" class="form-control"  name="lname" placeholder="<%= data[0].lname%>" required>
							</div><br><br>
						<label class="control-label col-lg-4">Middle Name:</label>
							<div class="col-lg-8">
								<input type="text" class="form-control"  name="mname" placeholder="<%= data[0].mname%>" required>
							</div><br><br>
						<label class="control-label col-lg-4 text-center">Educational Attainment:</label>
						<div class="col-lg-8">
							<select class='form-control' name='educationalAttainment'>
								<option value='Associate'>Associate</option>
								<option value='Bachelor'>Bachelor</option>
								<option value='Master'>Master</option>
								<option value='Doctorate'>Doctorate</option>
							</select>	
						</div><br><br>
						
						<label class="control-label col-lg-4 text-center">Specialization:</label>
						<div class="col-lg-8">
							<input list='programList' class='form-control' name="specialization" placeholder="<%= result[0].specialization%>" required>
						</div><br><br>
						
						<label class="control-label col-lg-4 text-center">School Graduated:</label>
						<div class="col-lg-8">
							<input list='schoolList' class='form-control' name="schoolGraduated" placeholder="<%= result[0].schoolGraduated%>" required>
						</div><br><br>
						
						<label class="control-label col-lg-4 text-center">Year Graduated:</label>
						<div class="col-lg-8">
							<input type="number" class="form-control yeargrad" id="yearGraduated" name="yearGraduated" placeholder="<%= result[0].yearGraduated%>" required>
						</div><br><br>
						
						<label class="control-label col-lg-4 text-center">Date of Birth:</label>
						<div class="col-lg-8">
							<input type="date" class="form-control datebirth" name="dateOfBirth" placeholder="<%= result[0].dateOfBirth%>" min="1910-01-01" required>
						</div><br><br>
					</div>
					
				

			</div>
			<div class="modal-footer">
			<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</form></div>
		</div>
	</div>
</div>


<!-----CHANGE PASSWORD----!>

<div id="changePassword" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Change Password</h4>
			</div>
			<div class="modal-body">

				<form class="form-horizontal" action="/changePassword" method="POST">
					<div class="form-group">

						<div class="col-lg-9">
							<input type='hidden' value='<%= data[0].facultyId %>' name='facultyId' >
						</div><br>

						<label class="control-label col-lg-3">Old Password:</label>
							<div class="col-lg-9">
								<input type="password" class="form-control"  name="oldPassword" required title="Must match old password">
							</div><br><br>
						<label class="control-label col-lg-3">New Password:</label>
							<div class="col-lg-9">
								<input type="password" class="form-control"  name="password" pattern=".{8,}" required title="Minimum of 8 characters">
							</div><br><br>
							<div class="col-lg-9 col-lg-offset-3">
								<p class="bg-light text-dark fontsize2">Note:  Password should be atleast 8 characters</p>
							</div>
					</div>
					
				

			</div>
			<div class="modal-footer">
				<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
				</form>
			</div>
		</div>
	</div>
</div>

