						<div class="form-group">
							<input type='hidden' name='value1' value='<%= data.value1 %>'>
							<input type='hidden' name='value2' value='<%= data.value2 %>'>

							<label class="control-label col-lg-4 text-center">Presentation:</label>
							<div class="col-lg-8">
								<input type="text" class="form-control"  value="<%= data.presentationName %>" name="presentationName" required>
							</div><br><br>

							<label class="control-label col-lg-4 text-center">Paper Name:</label>
							<div class="col-lg-8">
								<input type="text" class="form-control" value="<%= data.paperName %>" name="paperName" required>
							</div><br><br>

							<label class="control-label col-lg-4 text-center">Date:</label>
							<div class="col-lg-8">
								<input type="date" class="form-control" value="<%= data.date %>" name="confdate" required>
							</div><br><br>
						</div>
				</div>
			</div>
			<div class="modal-footer">
			<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</form></div>
		</div>
	</div>
</div>
<% }); %>

<!----view presentation----!>

<div id="viewPresentation" class="modal fade" role="dialog">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">View Presentation</h4>
			</div>
			<div class="modal-body">
				<table id='researchpendingTable' class='table table-striped'>
					<thead>
						<tr>
							<th>Presentation Name</th>
							<th>Paper Name</th>
							<th>Date</th>
							<th>Status</th>
						</tr>
					</thead>
					<tbody>
						<% pendingpres.forEach(function(data){ %>
						<tr>
							<td><%= data.presentationName %></td>
							<td><%= data.paperName %></td>
							<td><%= data.date %></td>
							<td>
								<% if( data.presentVerificationLevel == '1' && data.presentDenyStatus == '0'){ %>
									Approved
								<% }else if(data.presentVerificationLevel == '0' && data.presentDenyStatus == '1'){ %>
									Denied
								<% }else if(data.presentVerificationLevel == '0' && data.presentDenyStatus == '0'){ %>
									Pending
								<% }else if(data.presentVerificationLevel == '1' && data.presentDenyStatus == '1'){ %>
									Deleted
								<% } %>
							</td>
						</tr>
						<% }); %>
					</tbody>
				</table>

			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
