<!-- INSTITUTION --!>
<script src="js/Chart.js" charset="utf-8"></script>


<div id="addInstitution" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Add Institution</h4>
			</div>
			<div class="modal-body">
			 <ul id="myTab" class="nav nav-tabs">
                <li><a href="#basicInfo" data-toggle="tab" role="tab" id="tab1">Basic Information</a></li>
                <li><a href="#adminInfo" data-toggle="tab">Institution Admin Information</a></li>
            </ul>
			<form encType="multipart/form-data" class="form-horizontal" action="/addInstitution" method="POST">

            	<div id="myTabContent" class="tab-content" id="tab1">
            	<br>
             	 <div class="tab-pane fade" id="basicInfo" >
					<div class="form-group">
						<label class="control-label col-lg-4">Institution Logo:</label>
						<div class="col-lg-8">
							<input type="file" class="form-control" name="institutionLogo" accept=".jpg" title='Only .jpg files' required>
						</div><br><br>
					
						<label class="control-label col-lg-4">Name:</label>
						<div class="col-lg-8">
							<input type="text" class="form-control" placeholder="Enter name" name="institutionName" required>
						</div><br><br>
						<label class="control-label col-lg-4">Address:</label>
						<div class="col-lg-8">
							<input type="text" class="form-control" placeholder="Enter address" name="institutionAddress" required>
						</div><br><br>
						<label class="control-label col-lg-4 text-center">Region:</label>
						<div class="col-lg-8">
							<select class='form-control' name='region_id'>
								<% regName.forEach(function(data){ %>
									<option value='<%= data.region_id %>'><%= data.regionName %></option>
								<% }); %>
							</select>

						</div>
					</div>
						
					
			  	 </div>
			  	 <div class="tab-pane fade" id="adminInfo">
			  	 <br>
			  	  <div class="form-group">
			  	 	<label class="control-label col-lg-3">Profile Image:</label>
						<div class='col-lg-9'>
						<input type='file' class='form-control' name='profileImage' accept=".jpg" title='Only .jpg files' required>
						</div><br><br>

						<label class="control-label col-lg-3">First Name:</label>
						<div class="col-lg-9">
							<input type="text" class="form-control" id="fname" name="fname" placeholder="First Name" required>
						</div><br><br>
						<label class="control-label col-lg-3">Last Name:</label>
							<div class="col-lg-9">
								<input type="text" class="form-control" id="lname" name="lname" placeholder="Last Name" required>
							</div><br><br>
						<label class="control-label col-lg-3">Middle Name:</label>
							<div class="col-lg-9">
								<input type="text" class="form-control" id="mname" name="mname" placeholder="Middle Name">
							</div><br><br>
						<label class="control-label col-lg-3 text-center">Position:</label>
						<div class="col-lg-9">
            				<select class="form-control" name="position">
              						
					              <option value="Registrar">Registrar</option>
					           		
				            </select>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Degree:</label>
						<div class="col-lg-9">
							<select class='form-control' name='educationalAttainment'>
								<option value='Associate'>Associate</option>
								<option value='Bachelor'>Bachelor</option>
								<option value='Master'>Master</option>
								<option value='Doctorate'>Doctorate</option>
							</select>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Program:</label>
						<div class="col-lg-9">
							<input list='programList' class='form-control' name="programGraduated" placeholder="Program Graduated" required>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">School Graduated:</label>
						<div class="col-lg-9">
							<input list='schoolList' class='form-control' name="schoolGraduated" placeholder="School Graduated" required>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Year Graduated:</label>
						<div class="col-lg-9">
							<input type="number" min="1940" max="2020" class="form-control" id="yearGraduated" name="yearGraduated" placeholder="Year Graduated" class="dateInput" required>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Gender:</label>
						<div class="col-lg-9">
							<select class='form-control' name='gender'>
								<option value='Male'>Male</option>
								<option value='Female'>Female</option>
							</select>
						</div><br><br>
						<label class="control-label col-lg-3">Date of Birth:</label>
						<div class="col-lg-9">
								<input type="date" id="dob" name="dob" class='form-control' min="1910-01-01" required>
						</div><br><br>
						<label class="control-label col-lg-3 text-center">Faculty Type:</label>
						<div class="col-lg-9">
							<select class='form-control' name='facultyType'>
								<option value='Full-time'>Full-time</option>
								<option value='Part-time'>Part-time</option>
							</select>
						</div><br><br>
						<label class="control-label col-lg-3">Date Hired:</label>
						<div class="col-lg-9">
								<input type="date" id="DateHired" name="dateHired" class='form-control' min="1910-01-01" required>
						</div><br><br>
						<label class="control-label col-lg-3">Email:</label>
						<div class="col-lg-9">
							<input type="email" class="form-control" name="email" placeholder="Email" required>
						</div><br><br>
						<label class="control-label col-lg-3">Password:</label>
						<div class="col-lg-9">
							<input type="password" class="form-control" id="password" name="password" placeholder="Password" pattern=".{8,}" required title="Minimum of 8 characters">
						</div><br><br>
						<div class="col-lg-9 col-lg-offset-3">
							<p class="bg-light text-dark fontsize2">Note:  Password should be atleast 8 characters</p>
						</div>
						</div>
					</div>
			  	 </div>
			    

			</div>
			<div class="modal-footer">
			<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
				
				</form>
			</div>
		</div>
	</div>
</div>

<div id="editInstitution" class="modal fade" role="dialog">
