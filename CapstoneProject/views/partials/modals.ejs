			<div class="modal-footer">
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>

<div id="deleteInstitution" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Delete Institution</h4>
			</div>
			<div class="modal-body">
				<form class="form-horizontal" action="/deleteInstitution" method="POST">
					<div class="form-group">
						<label class="control-label col-lg-3">Name:</label>
						<div class="col-lg-9">
							<select class='form-control' name='institutionId'>
								<% data.forEach(function(data){ %>
									<option value='<%= data.institutionId %>'><%= data.institutionName %></option>
								<% }); %>
							</select>
						</div><br><br>
					</div>
					
				
			</div>
			<div class="modal-footer">
			<button type="submit" class="btn btn-primary">Submit</button>
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</form></div>
		</div>
	</div>
</div>


<!--LIST OF INSTITUTIONS MODALS--!>

<% institutiondata.forEach(function(data){ %>
<div id="institution<%= data.institutionId %>" class="modal fade" role="dialog">
	<div class="modal-dialog">
	<div class="modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<span class='close glyphicon glyphicon-remove' data-dismiss="modal"></span>
				<h4 class="modal-title">Institution Data</h4>
			</div>
			<div class="modal-body">

				<div class='row picbg'>
					<div class='col-lg-6 col-lg-offset-3 text-center'>
						<img class='face' src='../images/institution<%= data.institutionId %>.jpg'>
					</div>
				</div><br><br>
				<!-- institutionId, fname, lname, address, institutionName--!>
				<div class='row'>
					<div class='col-lg-4 text-center'>
						Institution Name:
					</div>
					<div class='col-lg-8'>
						<%= data.institutionName %> 
					</div><br><br>

					<div class='col-lg-4 text-center'>
						Address:
					</div>
					<div class='col-lg-8'>
						<%= data.address %>
					</div><br><br>

					<div class='col-lg-4 text-center'>
						Institution Admin:
					</div>
					<div class='col-lg-8'>
						<%= data.fname %> - <%= data.lname %>
					</div><br><br>

					<br>
					<div class='col-lg-4 text-center'>Certification</div>
					<% institutioncert.forEach(function(cdata){%>						
							<% if( cdata.institutionId == data.institutionId ){ %>
								<div class='col-lg-8 '>
									Center(s) of Development: <%= cdata.centerOfDevelopment %><br>
									Center(s) of Excellence: <%= cdata.centerOfExcellence %>
								</div><br><br>
							<% } %>
					<% }); %>

					<hr><br>
					<h4 style="text-align:center;">GRAPH</h4>
					<% distinctinstitution.forEach(function(cdata){%>						
							<% if( cdata.institutionId == data.institutionId ){ %>
								<div class='col-lg-12'>
									<canvas id="populationChart<%= cdata.institutionId %>"></canvas>
								</div>
							<% } %>
					<% }); %>
					
				</div>
				
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
			</div>
		</div>
		</div>
	</div>
</div>
<% }); %>





